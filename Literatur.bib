@article{vectorLQQ,
      title          = {Production of scalar and vector leptoquarks in e+e- annihilation},
      author         = {Blumlein and Ruckl},
      journal        = {Physics Letters B},
      volume         = {304},
      number         = {3},
      pages          = {337-346},
      year           = {1993},
      issn           = {0370-2693},
      doi            = {http://dx.doi.org/10.1016/0370-2693(93)90306-3},
      url            = {http://www.sciencedirect.com/science/article/pii/0370269393903063},
      abstract = {A systematic study in model-independent framework is presented for the production of scalar and vector leptoquarks in e+e- annihilation. We consider the most general coupling to the known fermions and gauge bosons which are dimensionless, baryon and lepton number conserving, family-diagonal, and SU(3)c \times SU(2)L \times U(1)Y invariant. Formal expressions are derived for production cross sections and differential distributions. Numerical predictions are discussed for LEP energies and beyond.}
}

